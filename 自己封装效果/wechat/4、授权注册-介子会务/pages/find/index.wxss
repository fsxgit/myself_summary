/* pages/find/index/index.wxss */
page{background: #f4f4f4;}
.c_fixed_top{ background: #fff;}
.search{ padding:16rpx 25rpx; background: #f4f4f4; border-radius: 8rpx; font-size:28rpx; color:#999; }
.search image{width:33rpx; height:auto; vertical-align: middle; margin-right:10rpx;}
.tab{ display: flex;}
.tab view{ flex: 1; text-align: center; padding:30rpx 0; font-size:32rpx; font-weight: 600;}
.tab view.active{ color:#4b8cf6; background: url("data:image/png;base64,") no-repeat center bottom; background-size:73rpx 6rpx;}
.list{ padding-top:1px; padding-bottom: 140rpx;}
.list .li{  margin-top:30rpx; background: #fff; padding-bottom: 20rpx;overflow: hidden; border-radius: 10rpx; box-shadow: 0 10rpx 10rpx rgba(0,0,0,0.1); position: relative; }
.status{ position: absolute; left:0; top:0; width:109rpx; height:54rpx; line-height: 54rpx; text-align: center; border-bottom-right-radius: 30rpx;  font-size:28rpx; color:#fff;}
.status1{background: #5b8cee;}
.status2{background: #d6554f;}
.status3{background: #c3c1c1;}
.list .bot{padding:0 20rpx;}
.list .img { width:100%; height:200px; overflow: hidden;}
.list .img image{ display: block; width:100%; height: 100%;}
.list .h2{overflow: hidden; text-overflow: ellipsis; white-space:nowrap; display: block; font-size:32rpx; color:#333; padding:24rpx 0 10rpx; }
.list .time1{ font-size:24rpx; color:#666; height:40rpx; line-height: 40rpx;}
.list .time1 image{vertical-align: middle; width:26rpx; height:auto; margin-right:15rpx;}
.list .time2{ font-size:24rpx; color:#666; height:40rpx; line-height: 40rpx;}
.list .time2 image{vertical-align: middle; width:26rpx; height:auto; margin-right:15rpx;}
.list .site{ font-size:24rpx; color:#666; height:40rpx; line-height: 40rpx;}
.list .site image{vertical-align: middle; width:24rpx; height:auto; margin-right:15rpx;}

.rzts_wrap{position: fixed; left:0; top:0; width: 100%; height: 100%; z-index: 10; background:rgba(0,0,0,0.5);}
.rzts{position: fixed; left:10%; top:calc(50% - 80px); background:#fff; border-radius: 20rpx; width:80%; }
.rzts .img{ padding: 15rpx 0;}
.rzts .img image{ width:121rpx; height: auto; display: block; margin:0 auto;  }
.rzts .txt{ text-align: center; width: 370rpx; font-size:32rpx; color:#666; margin:0 auto; }
.rzts .btns{ text-align: center; overflow: hidden; padding:36rpx 0;}
.rzts .btns .cancel{ width:40%; margin-left:5%; margin-right:5%; float: left; box-sizing: border-box; border:1px solid #4b8cf6; border-radius: 8rpx; color:#4b8cf6; height:86rpx; line-height: 86rpx; font-size: 28rpx; }
.rzts .btns .sure{ width:40%; margin-left:5%; margin-right:5%; float: left; border-radius: 8rpx; color:#fff; background: #4b8cf6; height:86rpx; line-height: 86rpx; font-size: 28rpx;}

/* pages/login/index.wxss */
.getUserInfo{ position: fixed; left:0; bottom:0; background: none; width:100%; height:100%; text-align: center; z-index: 12;}
.getUserInfo button:nth-of-type(1){ position: absolute; left:0; bottom:0; width:100%; height:100%; display: block; background: none; color:#fff; font-size: 0;   margin:0;padding:0;box-sizing: none; 
}
.getUserInfo button:nth-of-type(2){ position: absolute; right:0; bottom:0; width:100%; height:100%; display: block; background: none; color:#fff; font-size: 0;   margin:0;padding:0;box-sizing: none; 
}

.getMobileBox{position: fixed; left:0; top:0; width:100%; height: 100%; background:rgba(0,0,0,0.5); z-index: 12;}
.getMobileBox .center{position: fixed; left:10%; top:30%; width:80%; background: #fff; border-radius: 10px; box-sizing: border-box; padding:0 30rpx;}
.getMobileBox .center .h2{ text-align: center; font-size:14px; color:#333; padding:16px 0; font-weight: 600; }
.getMobileBox .center .item{ border:1px solid #e5e5e5; border-radius: 4px; height:88rpx; overflow: hidden; margin-bottom: 10px; }
.getMobileBox .input{ width: calc(100% - 168rpx); height:100%; float: left;}
.getMobileBox .input input{height:100%; width:100%; box-sizing: border-box; padding:0 20rpx;}
.getMobileBox .btn{ width: 168rpx;  float: right; font-size: 26rpx;  text-align: center; 
  /*弹性盒模型*/    
    display:flex;
/*主轴居中对齐*/
    justify-content: center;
/*侧轴居中对齐*/    
    align-items: center; 
    height:50rpx;
    border-left:1px solid #e5e5e5;
    box-sizing: border-box;
    margin-top:19rpx;
 }
.getMobileBox .wx{ color:#07bb06; background:none; padding:0;border-radius:0; min-height:10px;}
.getMobileBox .send{ color:#4b8cf6; }
.getMobileBox .wx image{ margin-right:4px; }
.getMobileBox .sqbtn{ width:100%;overflow: hidden; padding-bottom:40rpx;  }
.getMobileBox .sqbtn button{ width:100%; height:88rpx; text-align: center; font-size: 16px;  }
.getMobileBox .sqbtn .cancel{ width:48%; float: left;background: #aaa; color:#333; border-radius: 4px; overflow: hidden; }
.getMobileBox .sqbtn .sure{width:48%; float: right; background: #4b8cf6; color:#fff; border-radius: 4px; overflow: hidden;  }
.saoSQbox{position: fixed; left:0; top:0; width: 100%; height:100%; background: rgba(0,0,0,0.5); z-index:11; text-align: center; /*弹性盒模型*/    
    display:flex;
/*主轴居中对齐*/
    justify-content: center;
/*侧轴居中对齐*/    
    align-items: center;  }
.saoSQbox image{width:70%; z-index: 99;}