    $(".choose-xk").click(function(){
        $(this).toggleClass("active");

        var state = 1;
        $(".choose-xk-box .choose-xk").each(function(){
            if($(this).hasClass("active")){
                state = 1;
            }else{
                state = 2;
                return false;
            }
        });

        if(state == 1){
            $(".choose-xk-quan").addClass("active");
        }else{
            $(".choose-xk-quan").removeClass("active");
        }

        $(".choose-xk-box .choose-xk").each(function(){
            if($(this).hasClass("active")){
                $(this).find("input").attr("checked",true);
            }else{
                $(this).find("input").attr("checked",false);
            }
        });
    });


    $(".choose-xk-quan").click(function(){
        if($(this).hasClass("active")){
            $(this).removeClass("active");
            $(".choose-xk-box .choose-xk").removeClass("active");
        }else{
            $(this).addClass("active");
            $(".choose-xk-box .choose-xk").addClass("active");
        }

        $(".choose-xk-box .choose-xk").each(function(){
            if($(this).hasClass("active")){
                $(this).find("input").attr("checked",true);
            }else{
