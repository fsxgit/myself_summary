<script src="js/jquery-1.9.1.min.js"></script>
<script src="js/js.js"></script>
<script>
    $(function(){
//      二维码的显示隐藏
        maTab();

        var cTan = $(".c-tan");
        var tanBox1 = $(".tanBox1");
        var tanBox2 = $(".tanBox2");
        var zcCont = $(".zc-cont");
        var rlCont = $(".suc-cont");
        var oBtn = $(".tan-btn");
        var oClose = $(".c-tan-close");

//     弹出框居中显示隐藏
        oBtn.click(function(){
            var wW = cTan.width();
            var wH = cTan.height();
            var zcW = zcCont.width();
            var zcH = zcCont.height();
            var rlW = rlCont.width();
            var rlH = rlCont.height();

            var L1 = (wW - zcW)/2;
            var T1 = (wH - zcH)/2;
            var L2 = (wW - rlW)/2;
            var T2 = (wH - rlH)/2;
            zcCont.css({"left":L1,"top":T1});
            rlCont.css({"left":L2,"top":T2});
            tanBox1.fadeIn();
        });
        $(".btn-qu").click(function(){
            tanBox1.fadeOut();
        });
        $(".dingBtn1").click(function(){
            tanBox1.fadeOut();
            tanBox2.fadeIn();
        });
        $(".dingBtn2").click(function(){
            tanBox2.fadeOut();
        });
        oClose.click(function(){
            tanBox1.fadeOut();
            tanBox2.fadeOut();
        })


    })
</script>
