<script>
$(".g-xuan-box").each(function(){
    var i = $(this).index();
    var pd = 1;

//        单选
    $(".g-xuan-box").eq(i).find(".g-xuan").click(function(){
        $(this).toggleClass("active");
        //        给隐藏input添加取消选中事件
        if($(this).hasClass("active")){
            $(this).siblings("input").attr("checked",true);
        }else{
            $(this).siblings("input").attr("checked",false);
        }

        $(".g-xuan-box").eq(i).find(".g-xuan").each(function(){
            if($(this).hasClass("active")){
                pd = 1;
            }else{
                pd = 2;
                return false;
            }
        });
        if( pd == 1 ){
            $(".g-xuan-box").eq(i).find(".g-xuan-all").addClass("active");
        }else if( pd == 2 ){
            $(".g-xuan-box").eq(i).find(".g-xuan-all").removeClass("active");
        }
    });

//        全选
    $(".g-xuan-box").eq(i).find(".g-xuan-all").click(function(){
        $(this).toggleClass("active");
        if($(this).hasClass("active")){
            $(".g-xuan-box").eq(i).find(".g-xuan").each(function(){
                $(this).addClass("active");
            });
            $(".g-xuan-box").eq(i).find("input").each(function(){
                $(this).attr("checked",true);
            })
        }else{
            $(".g-xuan-box").eq(i).find(".g-xuan").each(function(){
                $(this).removeClass("active");
            });
            $(".g-xuan-box").eq(i).find("input").each(function(){
                $(this).attr("checked",false);
            })
        }


    })
});

$(".remove").click(function(){
   $(this).parents(".hang-lei-mode").remove();
});

</script>
