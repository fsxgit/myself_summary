<script>
    $(function(){
//      二维码的显示隐藏
        maTab();
        $(".quan-wai").click(function(){
            $(this).addClass("active").siblings().removeClass("active");
        })

    });
//    修改名字弹出框
    function xgName(){
        $(".xg-name").fadeIn();
        $(".c-tan-close,.xg-sure").click(function(){
            $(".xg-name").fadeOut();
        })
    }

//查看详情弹出框

    function ckdetail(){
        var cTan = $(".c-tan");
        var tanBox1 = $(".brand-tan");
        var tanBox2 = $(".brand-kan");

        var bdCont = $(".sb-cont");
        var ckCont = $(".ck-cont");

        var oClose = $(".c-tan-close");

//     弹出框居中显示隐藏
        var wW = cTan.width();
        var wH = cTan.height();
        var zcW = bdCont.width();
        var zcW2 = ckCont.width();
        var zcH = bdCont.height();
        var zcH2 = ckCont.height();


        var L1 = (wW - zcW)/2;
        var T1 = (wH - zcH)/2;
        var L2 = (wW - zcW2)/2;
        var T2 = (wH - zcH2)/2;
        bdCont.css({"left":L1,"top":T1});
        ckCont.css({"left":L2,"top":T2});
        tanBox1.fadeIn();
        $(".ck-btn").click(function(){
            tanBox1.fadeOut();
            tanBox2.fadeOut();

        });

        $(".ckxq").click(function(){
            tanBox1.fadeOut();
            tanBox2.fadeIn();
        })
        oClose.click(function(){
            tanBox1.fadeOut();
            tanBox2.fadeOut();
        })

    }

</script>
